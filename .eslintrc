{
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["react", "prettier"],
    "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
    "rules": {
        "semi": 1,
        "quotes": ["error", "single"],
        "react/prop-types": 1,
        "prettier/prettier": 2,
        "react/jsx-uses-vars": 2
    }
}
